{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python: FastAPI",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/app/main.py",
			"args": ["run"],
			"console": "integratedTerminal",
			"justMyCode": true,
			"env": {
				"PYTHONPATH": "${workspaceFolder}"
			},
			"pythonArgs": ["-Xfrozen_modules=off"]
		}
	]
}
